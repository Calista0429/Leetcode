   "source": [
    "nums = [-1,0,3,5,9,12]\n",
    "target = 5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "234b33f2-58ac-4bba-a86f-554c2cc8ab1a",
   "metadata": {},
   "outputs": [],
   "source": [
    "left = 0\n",
    "right = len(nums) - 1   \n",
    "while left <= right:\n",
    "    mid = (left + right) // 2\n",
    "    if nums[mid] > target:\n",
    "        right = mid - 1\n",
    "    elif nums[mid] < target:\n",
    "        left = mid + 1\n",
    "    else :\n",
    "        return mid\n",
    "return -1"
   ]
  }
 ],
